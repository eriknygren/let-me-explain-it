<div class="modal-header">
    <h3>{{ title }}</h3>
</div>
<div class="modal-body">
    <p>{{ content }}</p>
    <input ng-show="showEasyCopyContent" onClick="this.select();" class="prompt-dialog standard-cursor" ng-model="easyCopyContent" ng-readonly="true" />
</div>
<div class="modal-footer">
    <button ng-click="close()" class="btn">Ok</button>
</div>
